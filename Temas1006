1
SELECT id_product, SUM(qty) AS total
FROM product_order
GROUP BY id_product
ORDER BY total DESC;

2
SELECT id_product, price
FROM product_order
WHERE id_order = 1
ORDER BY price DESC;

3
SELECT id_product, price * qty AS total
FROM product_order
WHERE id_order = 2
ORDER BY total DESC;

4
SELECT UPPER(name)
FROM product;

5
SELECT city.`name`, state.`name` FROM city
LEFT JOIN state ON city.id_state = state.id
order BY city.name;

6
SELECT city.`name`, state.`name` FROM city
LEFT JOIN state ON city.id_state = state.id
WHERE city.name LIKE "%igua√ßu%"
order BY city.name;